<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aiyang.allhttp_networkrequestdemo">



    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="false"
        android:icon="@mipmap/as"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".feature.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".feature.HttpClientActivity" />
        <activity android:name=".feature.HttpUrlConnection"/>
        <activity android:name=".io.NetworkActivity"/>
        <activity android:name=".feature.VolleyActivity"/>
        <activity android:name=".feature.OkHttp3Activity"/>
        <activity android:name=".feature.RetrofitActivity"/>
        <activity android:name=".feature.RxJavaActivity"/>
    </application>

</manifest>